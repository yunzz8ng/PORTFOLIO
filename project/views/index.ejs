    <script>
        function openlogin() {
            const options = "width=250, height=250, top=50%, left= 50%";
            window.open("http://localhost:3000/login", "_blank", options);
        }

        function openPost() {
            const options = "width=950, height=450, top=50%, left= 50%";
            window.open("http://localhost:3000/posts", "_blank", options);
        }

        function updateUser() {
            const options = "width=300, height=500, top=50%, left= 50%";
            window.open("http://localhost:3000/edit", "_blank", options);
        }

        function openMap() {
            const options = "width=300, height=500, top=50%, left= 50%";
            window.open("http://localhost:3000/korea", "_blank", options);
        }

        function openMe() {
            const options = "width=1000, height=1000, top=50%, left= 50%";
            window.open("https://yunzzang-c.tistory.com/", "_blank", options);
        }
    </script>
    <head>
        <meta charset="UTF-8" />
