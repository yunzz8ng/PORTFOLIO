        * {
            margin: 0;
            padding: 0;
        }
        body {
            width: 100%;
            height: 100vh;
            overflow: hidden;
        }
        header {
            margin: auto;
            width: 100%;
            height: 5em;
            background-image: linear-gradient(to right, #010081, #008080);
            display: flex;
            align-items: center;
            justify-content: space-between;
            border-width: 0.3em;
            border-color: #c3c3c3;
            border-style: groove;
            font-family: "DungGeunMo";
            font-size: 1em;
        }
        footer {
            width: 100%;
            height: auto;
            display: flex;
            justify-content: space-between;
        }
        #logo {
            padding-left: 1.6em;
            margin-top: 0.09em;
            width: 4.7em;
            height: 4.7em;
        }
        .nav {
            list-style: none;
            display: flex;
            padding-right: 1em;
        }
        .nav > li {
            padding: 0.5em;
            height: 0.5em;
            line-height: 0.5em;
            width: 3em;
            text-align: center;
            border-style: outset;
            background-color: #c3c3c3;
            box-shadow: 1px 1px 1px 1px rgb(67, 67, 67);
            margin: 0.5em;
        }

        .nav > li > a {
            text-decoration: none;
            color: inherit;
        }
        .container {
            border-width: 0.3em;
            border-color: #c3c3c3;
            border-style: groove;
            width: 100%;
            height: 85vh;
        }

        .main {
            background-color: #008080;
            width: 70%;
            height: 85vh;
            float: left;
        }
        .right {
            float: right;
            border-style: outset;
            width: 29.7%;
            height: 40vh;
            padding: 0;
            margin: 0;
            border-width: 0.1em;
        }
        #right-bar {
            width: 100%;
            height: 1em;
            margin: 0;
            padding: 0;
            background-image: linear-gradient(to right, #818181, #c3c3c3);
            display: flex;
            align-items: center;
            justify-content: space-between;
            border-color: #c3c3c3;
            border-style: groove;
            border-width: 0.2em;
            font-family: "DungGeunMo";
            font-size: 0.7em;
        }
        #right-bar-menu {
            color: #c3c3c3;
        }
        .start {
            width: 5%;
            height: 100%;
            background-color: #c3c3c3;
            border-width: 0.3em;
            border-style: groove;
            justify-content: center;
            align-items: center;
            display: flex;

            position: relative;
        }
        .start-content {
            display: none;
            position: absolute;
            left: 0;
            top: auto;
            bottom: 100%;
            width: 200%;
            z-index: 1;
            border: 1px outset #ccc;
            box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);

            overflow: auto;
        }
        .start-content a {
            font-family: "DungGeunMo";
            padding: 12px 16px;
            text-decoration: none;
            display: block;
            color: black;
            background-color: #c3c3c3;
            border-bottom: 2px groove #e5e5e5;
        }

        .start-content a:last-child {
            border-bottom: none; /* Remove the bottom border for the last link */
        }

        .start-content a:hover {
            background-color: #818181;
        }

        /* Show the start when hovering over the .start container */
        .start:hover .start-content {
            display: block;
        }
        .start button::after {
            content: ""; /* Clear content */
            border-left: 5px outset transparent; /* Left arrow slant */
            border-right: 5px outset transparent; /* Right arrow slant */
            border-top: 5px outset #000; /* Top border color and size */
            display: inline-block; /* Allow it to sit next to the text */
            margin-left: 10px; /* Space between the text and the arrow */
            margin-top: -5px;
            vertical-align: middle; /* Vertically align the arrow */
        }

        .start button {
            padding: 8px 16px;
            border: 1px outset #ccc;
            border-radius: 4px;
            background-color: #c3c3c3;
            cursor: pointer;
            font-size: 14px;
            outline: none;
            transition: background-color 0.2s;
        }
        .start > img {
            width: 3.5em;
            height: auto;
            padding: auto;
            margin: 0;
        }
        .start:hover {
            border-style: inset;
            border-width: 0.3em;
        }

        .main > div {
            padding: 1.8em;
        }

        .icons {
            width: 10%;
            height: 3em;
            text-align: center;
            display: flex; /* Flexbox 사용 */
            flex-direction: column; /* 수직 정렬 */
            justify-content: center; /* 세로 중앙 정렬 */
            align-items: center; /* 가로 중앙 정렬 */
            padding: 1em;
            margin: 2em;
            margin-top: 3em;
            font-family: "DungGeunMo";
            font-size: 1.2em;
        }
        .icons > img {
            display: block;
            padding: 1em;
            width: 3.5em;
            height: 3.5em;
        }
        #time {
            width: 10%;
            font-size: 1.5em;
            align-items: center;
            justify-content: center;
            display: flex;
            background-color: #c3c3c3;
            border-style: groove;
            border-width: 2px;
        }
        .loading {
            background-color: #c3c3c3;
            width: 85%;
            border-style: groove;
        }
        #juso > input {
            border-style: outset;
            border-width: 0.1em;
            height: 0.7em;
        }
        #juso > input:focus {
            outline: none;
        }
    </style>
